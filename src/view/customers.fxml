<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="1144.0" style="-fx-background-color: #FFF;" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CustomersController">
    <children>
        <TableView fx:id="customerTable" layoutX="372.0" layoutY="68.0" prefHeight="450.0" prefWidth="755.0">
            <columns>
                <TableColumn fx:id="customerIdCol" text="Customer ID" />
                <TableColumn fx:id="nameCol" prefWidth="120.0" text="Name" />
                <TableColumn fx:id="countryCol" prefWidth="62.0" text="Country" />
                <TableColumn fx:id="firstLvlDivCol" prefWidth="124.0" text="First-Level Division" />
                <TableColumn fx:id="addressCol" prefWidth="186.0" text="Address" />
                <TableColumn fx:id="postalCodeCol" prefWidth="81.0" text="Postal Code" />
                <TableColumn fx:id="phoneNumCol" prefWidth="101.0" text="Phone Number" />
            </columns>
        </TableView>
        <AnchorPane prefHeight="538.0" prefWidth="355.0" style="-fx-background-color: #0C39FF;">
            <children>
                <Text fill="WHITE" layoutX="30.0" layoutY="61.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Customer Records">
                    <font>
                        <Font name="System Bold" size="33.0" />
                    </font>
                </Text>
                <Button fx:id="backButton" layoutX="122.0" layoutY="481.0" mnemonicParsing="false" onAction="#onBackButtonClickHandler" prefHeight="35.0" prefWidth="98.0" text="Back" />
                <Button fx:id="addButton" layoutX="100.0" layoutY="138.0" mnemonicParsing="false" onAction="#onAddCustomerHandler" prefHeight="43.0" prefWidth="141.0" text="Add Customer" />
            </children>
        </AnchorPane>
        <Label fx:id="errorLabel" alignment="TOP_LEFT" layoutX="372.0" layoutY="14.0" prefHeight="43.0" prefWidth="456.0" wrapText="true" />

    </children>
</AnchorPane>
